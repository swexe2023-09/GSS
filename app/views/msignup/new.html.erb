<h2>マネージャーを登録する</h2>

<%= form_tag '/msignup/create', method: :post, data: { turbo: false } do %>
<p>
    <%= label_tag :uid, 'ユーザーID' %>
    <%= text_field_tag :uid %>
</p>
<p>
    <%= label_tag :gid, 'グループID' %>
    <%= text_field_tag :gid %>
</p>
<p>
    <%= label_tag :password, 'パスワード' %>
    <%= password_field_tag :password %>
</p>
<p>
    <%= label_tag :password_confirmation, 'パスワード(確認)' %>
    <%= password_field_tag :password_confirmation %>
</p>
<p>
    <%= submit_tag '登録' %>
</p>
<% end %>


<br>
<%= link_to "ユーザー一覧に戻る" ,:back %>
<br>
<%= link_to "トップに戻る" , root_path %>